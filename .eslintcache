ebProject/React/ec-app/src/index.js",[],["227","228"],"/Users/macbook/Documents/WebProject/React/ec-app/src/reportWebVitals.js",[],"/Users/macbook/Documents/WebProject/React/ec-app/src/App.jsx",[],"/Users/macbook/Documents/WebProject/React/ec-app/src/reducks/store/store.js",["229"],"import {\n    createStore as reduxCreateStore,\n    combineReducers,\n    applyMiddleware\n} from 'redux';\nimport {createLogger} from 'redux-logger/src';\nimport {connectRouter, routerMiddleware} from 'connected-react-router';\nimport thunk from 'redux-thunk';\n\nimport {ProductsReducer} from '../products/reducers';\nimport {UsersReducer} from '../users/reducers';\nimport {LoadingReducer} from '../loading/reducers';\n\nexport default function createStore(history) {\n    const logger = createLogger({\n        collapsed: true,\n        diff: true\n    });\n    return reduxCreateStore(\n        combineReducers({\n            router: connectRouter(history),\n            loading: LoadingReducer,\n            users: UsersReducer,\n            products: ProductsReducer\n        }),\n        applyMiddleware(\n            routerMiddleware(history),\n            thunk\n        )\n    )\n}","/Users/macbook/Documents/WebProject/React/ec-app/src/reducks/users/reducers.js",[],"/Users/macbook/Documents/WebProject/React/ec-app/src/reducks/store/initialState.js",[],"/Users/macbook/Documents/WebProject/React/ec-app/src/reducks/users/actions.js",[],"/Users/macbook/Documents/WebProject/React/ec-app/src/Router.jsx",[],"/Users/macbook/Documents/WebProject/React/ec-app/src/templates/index.js",[],"/Users/macbook/Documents/WebProject/React/ec-app/src/templates/Home.jsx",[],["230","231"],"/Users/macbook/Documents/WebProject/React/ec-app/src/reducks/users/selectors.js",[],"/Users/macbook/Documents/WebProject/React/ec-app/src/templates/SignUp.jsx",[],"/Users/macbook/Documents/WebProject/React/ec-app/src/components/UIkit/index.js",[],"/Users/macbook/Documents/WebProject/React/ec-app/src/firebase/index.js",[],"/Users/macbook/Documents/WebProject/React/ec-app/src/components/UIkit/TextInput.jsx",[],"/Users/macbook/Documents/WebProject/React/ec-app/src/components/UIkit/PrimaryButton.jsx",[],"/Users/macbook/Documents/WebProject/React/ec-app/src/firebase/config.js",[],"/Users/macbook/Documents/WebProject/React/ec-app/src/reducks/loading/reducers.js",[],"/Users/macbook/Documents/WebProject/React/ec-app/src/reducks/loading/actions.js",[],"/Users/macbook/Documents/WebProject/React/ec-app/src/reducks/users/operations.js",[],"/Users/macbook/Documents/WebProject/React/ec-app/src/function/common.js",["232"],"/**\n * Validate email input\n * @param email\n * @returns {boolean}\n */\nexport const isValidEmailFormat = (email) => {\n    const regex = /^[a-zA-Z0-9.!#$%&'*+\\/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\\.[a-zA-Z0-9-]+)*$/;\n    return regex.test(email);\n}\n\n/**\n * Show alert if required is blank\n * @param asrg Required values\n * @returns {boolean}\n */\nexport const isValidRequiredInput = (...args) => {\n    let validator = true;\n    for (let i=0; i<args.length; i=(i+1)|0) {\n        if (args[i] === \"\") {\n            validator = false;\n        }\n    }\n    return validator;\n}","/Users/macbook/Documents/WebProject/React/ec-app/src/reducks/loading/selectors.js",[],"/Users/macbook/Documents/WebProject/React/ec-app/src/templates/SignIn.jsx",[],"/Users/macbook/Documents/WebProject/React/ec-app/src/templates/Reset.jsx",[],"/Users/macbook/Documents/WebProject/React/ec-app/src/Auth.jsx",["233"],"import React, { useEffect } from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { listenAuthState } from './reducks/users/operations';\nimport { getIsSignedIn } from './reducks/users/selectors';\n\nconst Auth = ({children}) => {\n    const dispatch = useDispatch();\n    const selector = useSelector((state) => state);\n    const isSignedIn = getIsSignedIn(selector);\n\n    useEffect(() => {\n        if (!isSignedIn) {\n            dispatch(listenAuthState());\n        }\n    }, [])\n\n    if (!isSignedIn) {\n        return <></>\n    } else {\n        return children;\n    }\n}\n\nexport default Auth","/Users/macbook/Documents/WebProject/React/ec-app/src/components/UIkit/SelectBox.jsx",[],"/Users/macbook/Documents/WebProject/React/ec-app/src/templates/ProductEdit.jsx",[],"/Users/macbook/Documents/WebProject/React/ec-app/src/reducks/products/reducers.js",[],"/Users/macbook/Documents/WebProject/React/ec-app/src/reducks/products/actions.js",[],"/Users/macbook/Documents/WebProject/React/ec-app/src/reducks/products/operations.js",["234","235"],"/Users/macbook/Documents/WebProject/React/ec-app/src/components/Products/ImageArea.jsx",["236","237"],"import React, { useCallback } from 'react';\nimport IconButton from '@material-ui/core/IconButton';\nimport AddPhotoAlternateIcon from '@material-ui/icons/AddPhotoAlternate';\nimport { makeStyles } from '@material-ui/core';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { hideLoadingAction, showLoadingAction } from '../../reducks/loading/actions';\nimport {storage} from '../../firebase/index';\nimport ImagePreview from './ImagePreview';\nimport { getLoadingText } from '../../reducks/loading/selectors';\n\nconst useStyles = makeStyles({\n    \"icon\": {\n        height: 48,\n        width: 48\n    }\n})\n\nconst ImageArea = (props) => {\n    const classes = useStyles();\n    const dispatch = useDispatch();\n    const selector = useSelector((state) => state);\n    const loadingState = getLoadingText(selector);\n    const loadingText = getLoadingText(selector);\n\n    const deleteImage = useCallback(async(id) => {\n        const ret = window.confirm('この画像を削除しますか？');\n        if (!ret) {\n            return false;\n        } else {\n            const newImage = props.images.filter(image => image.id !== id);\n            props.setImages(newImage);\n            return storage.ref('images').child(id).delete();\n        }\n    }, [props.images]);\n\n    const uploadImage = useCallback((event) => {\n        dispatch(showLoadingAction(\"uploading...\"));\n        const file = event.target.files;\n        let blob = new Blob(file, {type: \"image/jpeg\"});\n\n        // Generate random 16 digits strings\n        const S = \"abcdefghijklmnopgrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789\";\n        const N = 16;\n        const fileName = Array.from(crypto.getRandomValues(new Uint32Array(N))).map((n)=>S[n%S.length]).join('');\n\n        const uploadRef = storage.ref('images').child(fileName);\n        const uploadTask = uploadRef.put(blob);\n\n        uploadTask.then(() => {\n            // Handle sucessful uploads on complete\n            uploadTask.snapshot.ref.getDownloadURL().then((downloadURL) => {\n                const newImage = {id: fileName, path: downloadURL};\n                props.setImages((prevState => [...prevState, newImage]));\n                dispatch(hideLoadingAction());\n            });\n        }).catch(() => {\n            dispatch(hideLoadingAction());\n        });\n    }, [props.setImages]);\n    return (\n        <div>\n            <div className=\"p-grid__list-images\">\n                {props.images.length > 0 && (\n                    props.images.map(image => <ImagePreview delete={deleteImage} id={image.id} path={image.path} key={image.id} />)\n                )}\n            </div>\n            <div className=\"u-text-right\">\n                <span>商品画像を登録する</span>\n                {loadingState && (\n                    <div className=\"c-section__loading\">\n                        <p>{loadingText}</p>\n                    </div>\n                )}\n                <IconButton className={classes.icon}>\n                    <label>\n                        <AddPhotoAlternateIcon />\n                        <input \n                            className=\"u-display-none\" type=\"file\" id=\"image\"\n                            onChange={(event) => uploadImage(event)}\n                        />\n                    </l